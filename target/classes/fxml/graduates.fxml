<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="479.0" prefWidth="817.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cole.controller.GraduatesController">
   <children>
      <HBox alignment="CENTER" prefHeight="18.0" prefWidth="600.0">
         <children>
            <Label text="Graduate Students">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="27.0" prefWidth="800.0" spacing="10.0">
         <children>
            <Button fx:id="bukRequestButton" mnemonicParsing="false" text="Bulk Transcript Request" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
      <TableView fx:id="graduationTable" editable="true" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="numberColumn" text="Student Number" />
            <TableColumn fx:id="nameColumn" text="Student Name" />
            <TableColumn fx:id="idNumberColumn" text="ID Number" />
            <TableColumn fx:id="slpColumn" text="SLP Course" />
            <TableColumn fx:id="emailColumn" text="Email" />
            <TableColumn fx:id="phoneColumn" text="Phone" />
            <TableColumn fx:id="branchColumn" text="Branch" />
            <TableColumn fx:id="transcriptColumn" text="Transcript Requested" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
</VBox>
